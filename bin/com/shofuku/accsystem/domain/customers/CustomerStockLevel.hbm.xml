<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.shofuku.accsystem.domain.customers.CustomerStockLevel"
		table="t205_customer_stock_level">


	  <meta attribute="class-description">
         This class contains the customer stock level records. 
      </meta>
      
     <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      
      <property name="stockLevel" column="STOCK_LEVEL" type="double"/>
      <property name="itemCode" column="ITEM_CODE" type="string"/>
      
      
	</class>
</hibernate-mapping>